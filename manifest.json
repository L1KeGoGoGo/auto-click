{
  "name": "Auto Click [beta	]",
  "short_name": "Auto Click",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Auto click like a PRO :) [Donate to Support]",
  "default_locale": "en",
  "permissions": [
    "storage",
	"notifications",
	"contextMenus",
	"bookmarks"
  ],
  "icons": { 
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_32.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["jquery-3.1.1.min.js","utility.js","auto.click.js"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html"
}